{
  "manifest_version": 2,
  "name": "Show answers!",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "web_accessible_resources": [
    "hash_id.json"
]
}
